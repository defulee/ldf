 #!/bin/bash

 ### start btrace debugger

 base_dir=$(dirname "$(echo "$0" | sed -e '')")
 if [ $# -eq 0 ];then
     echo "Usage : wtool btrace btrace [-p <port>] [-cp <classpath>] <pid> <btrace-script> [<args>]"
     echo "More details about btrace please read doc: $base_dir/btrace.lib/README.md"
     echo "Site: https://github.com/btraceio/btrace/wiki"
     exit 1;
 fi

 sh $base_dir/btrace.lib/bin/btrace $@
